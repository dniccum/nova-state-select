{
    "name": "dniccum/nova-state-select",
    "description": "A Laravel Nova field that is a select dropdown pre-populated with all of the states within the US.",
    "keywords": [
        "laravel",
        "nova",
        "state",
        "select"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dniccum\\StateSelect\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dniccum\\StateSelect\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
